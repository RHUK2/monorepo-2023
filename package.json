{
  "name": "monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "shared/*"
  ],
  "scripts": {
    "dev:service": "npm run dev -w apps/service",
    "dev:admin": "npm run dev -w apps/admin",
    "build:service": "npm run build -w apps/service",
    "build:admin": "npm run build -w apps/admin",
    "start:service": "npm run start -w apps/service",
    "start:admin": "npm run start -w apps/admin"
  },
  "dependencies": {
    "@shared/components": "^0.1.0",
    "@shared/utils": "^0.1.0",
    "@shared/hooks": "^0.1.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.3"
  }
}
